        {{#ifEquals user.cust_type "customer"}}
        <div id="chatbotdiv">
          <button id="chatbtn" onclick="displayChatBot()"><i class="fas fa-robot"></i></button>
          <iframe src="/chatbot/chatbotlayout" id="chatbotiframe"></iframe>
        </div>
        <script>
          let chatbotframe = document.getElementById("chatbotiframe");

          chatbotframe.style.display = "none";
          let chatbtn = document.getElementById("chatbtn");

          function sleep(delay) {
            var start = new Date().getTime();
            while (new Date().getTime() < start + delay);
          }

          function displayChatBot(){
            chatbtn.style.display = "none";
            chatbotframe.src = "/chatbot/chatbotlayout";
            sleep(800);
            chatbotframe.style.display = "block";
          }
          
          function closeBotChat(){
            chatbotframe.style.display = "none";
            chatbtn.style.display = "block";
          }
        
          
          
        </script>
        {{/ifEquals}}
